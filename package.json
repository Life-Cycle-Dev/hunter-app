{
  "name": "hunter",
  "version": "1.0.0",
  "scripts": {
    "local": "BACKEND_PATH=http://localhost:9000 expo start -c",
    "dev": "BACKEND_PATH=https://hunter-dev-api.tongla.dev expo start -c",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "build:dev:android": "eas build -p android --profile dev --local",
    "build:dev:ios": "eas build -p ios --profile dev --local",
    "build:android": "eas build -p android --profile dev",
    "build:ios": "eas build -p ios --profile dev"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.10.0",
    "expo": "53.0.17",
    "expo-blur": "^14.1.5",
    "expo-clipboard": "^7.1.5",
    "expo-constants": "~17.1.6",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-status-bar": "~2.2.3",
    "nativewind": "latest",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-alert-notification": "^0.4.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "^10.2.0",
    "expo-dev-client": "~5.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "eslint": "^9.25.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3"
  },
  "main": "expo-router/entry",
  "private": true
}